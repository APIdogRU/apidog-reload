{
	"compilerOptions": {
        "jsx": "react",
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "es5",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"sourceMap": false,
		"outDir": "dist",
		"baseUrl": ".",
		"lib": [
			"dom",
			"es5"
		]
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules/*"
	]
}
